{
  "name": "cloudinary/media-management",
  "version": "0.1.1-beta",
  "description": "Cloudinary Media Management PHP SDK",
  "keywords": [
    "cloudinary",
    "sdk",
    "cloud",
    "image management",
    "cdn"
  ],
  "type": "library",
  "homepage": "https://github.com/cloudinary/media-management-php",
  "minimum-stability": "stable",
  "license": "MIT",
  "authors": [
    {
      "name": "Cloudinary",
      "homepage": "https://github.com/cloudinary/media-management-php/graphs/contributors"
    }
  ],
  "support": {
    "email": "info@cloudinary.com",
    "issues": "https://github.com/cloudinary/media-management-php/issues"
  },
  "require": {
    "php": ">=7.4.0",
    "cloudinary/transformation-builder-sdk":  "^1",
    "guzzlehttp/guzzle": "^6|^7",
    "shrikeh/teapot": "^1|^2",
    "ext-json": "*",
    "monolog/monolog": "^1|^2|^3",
    "psr/log" : "^1|^2|^3"
  },
  "require-dev": {
    "phpunit/phpunit": "^9",
    "phpmd/phpmd": "*",
    "squizlabs/php_codesniffer": "3.*",
    "friendsofphp/php-cs-fixer": "*",
    "ext-dom": "*",
    "ext-libxml": "*",
    "ext-zip": "*",
    "consolidation/robo": "~1"
  },
  "autoload": {
    "classmap": [
      "src"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Cloudinary\\MediaManagement\\Test\\": "tests"
    }
  }
}
